{
  "name": "titanis-cache",
  "version": "1.1.1",
  "description": "A lightweight LocalStorage caching solution with compression & expiration.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "localstorage",
    "cache",
    "typescript",
    "compression",
    "expiration"
  ],
  "author": "sdoerger",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdoerger/titanis-cache.git"
  },
  "bugs": {
    "url": "https://github.com/sdoerger/titanis-cache/issues"
  },
  "homepage": "https://github.com/sdoerger/titanis-cache#readme",
  "license": "MIT",
  "dependencies": {
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
